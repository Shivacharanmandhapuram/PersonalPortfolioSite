{
  "buildCommand": "vite build --config vite.config.production.ts --outDir dist/public",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/playbook-feed.js": {
      "runtime": "nodejs18.x"
    },
    "api/substack-feed.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}